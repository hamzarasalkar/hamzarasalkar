// practical no 5 write a program to accept the value of year as input from kyeword and print whether it is a leap year or not
#include <stdio.h>
void main()
{
int year;
printf("enter year\n");
scanf("%d",&year);
if(year%4==0)
{
    printf("it is a leap year");
}
else
{
    printf("it is not a leap year");
}
}
